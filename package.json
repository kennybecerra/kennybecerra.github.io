{
  "name": "portfolio-site",
  "version": "1.1.0",
  "description": "Portfolio site for Kenny",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel ./src/index.html",
    "prebuild": "rm -rf dist/",
    "build": "parcel build ./src/index.html --no-source-maps --public-url .",
    "postbuild": "npx ts-node scripts/deploy",
    "deploy": "npx ts-node -r dotenv/config  scripts/deploy"
  },
  "browserslist": [
    "last 5 versions"
  ],
  "keywords": [],
  "author": "Kenny Becerra",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0",
    "@parcel/transformer-sass": "^2.0.0",
    "@types/animejs": "^3.1.4",
    "@types/ssh2": "^0.5.48",
    "@types/ssh2-sftp-client": "^7.0.0",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "dotenv": "^10.0.0",
    "htmlnano": "^1.0.1",
    "parcel": "^2.0.0",
    "parcel-namer-custom": "^0.2.0",
    "postcss": "^8.3.6",
    "sass": "^1.38.0",
    "ssh2": "^1.5.0",
    "ssh2-sftp-client": "^7.1.0",
    "svgo": "^2.6.1",
    "terser": "^5.14.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "typewriter-effect": "^2.18.0"
  },
  "parcel-namer-custom": {
    ".css$": "css/[name].[hash].[type]",
    "(jpg|png|webp|ico)$": "assets/images/[name].[hash].[type]",
    "(ttf)$": "assets/fonts/[name].[hash].[type]",
    "(mp4|ogv|webm)$": "assets/videos/[name].[hash].[type]",
    "(pdf)$": "assets/documents/[name].[hash].[type]",
    "(svg)$": "assets/svg/[name].[hash].[type]"
  }
}
